{
    "name": "empari/laravel-support",
    "version": "5.5.2",
    "description": "Domain Tools, Utils and Support Package for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Team #EGI",
            "email": "suporte@empari.com.br"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "1.3.1",
        "doctrine/cache": "^1.7",
        "laravel/framework": "^5.5",
        "league/flysystem": "^1.0",
        "webpatser/laravel-uuid": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Empari\\Laravel\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
